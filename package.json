{
	"name": "svg-blur-up",
	"version": "1.1.6",
	"description": "A tool that generates image previews.",
	"homepage": "https://github.com/vanruesc/blur-up",
	"main": "build/svg-blur-up.js",
	"module": "src/index.js",
	"exports": {
		".": "./src/index.js"
	},
	"bin": {
		"blur-up": "./bin/cli.js"
	},
	"sideEffects": false,
	"license": "Zlib",
	"keywords": [
		"lazy",
		"preview",
		"image",
		"embed",
		"svg",
		"base64",
		"data",
		"uri"
	],
	"author": {
		"name": "Raoul van RÃ¼schen",
		"email": "vanruesc@outlook.de"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/vanruesc/blur-up.git"
	},
	"bugs": {
		"url": "https://github.com/vanruesc/blur-up/issues"
	},
	"files": [
		"src",
		"build",
		"bin"
	],
	"scripts": {
		"ava": "ava",
		"lint": "npm run lint:lib && npm run lint:test",
		"lint:config": "eslint *.js",
		"lint:lib": "eslint src",
		"lint:test": "eslint test",
		"build": "rollup -c",
		"build:production": "cross-env NODE_ENV=production npm run build",
		"pretest": "npm run lint && npm run build:production",
		"test": "ava",
		"prepack": "npm test && concat -o bin/cli.js shebang.txt bin/cli.js"
	},
	"ava": {
		"failFast": true,
		"files": [
			"test/test.js"
		],
		"require": [
			"esm"
		]
	},
	"eslintConfig": {
		"extends": "aether"
	},
	"dependencies": {
		"fs-extra": "9.x.x",
		"glob": "7.x.x",
		"sharp": "0.x.x",
		"yargs-parser": "20.x.x"
	},
	"devDependencies": {
		"@babel/core": "7.x.x",
		"@babel/preset-env": "7.x.x",
		"@rollup/plugin-babel": "5.x.x",
		"@rollup/plugin-node-resolve": "9.x.x",
		"ava": "3.x.x",
		"concat": "1.x.x",
		"cross-env": "7.x.x",
		"eslint": "7.x.x",
		"eslint-config-aether": "1.x.x",
		"esm": "3.x.x",
		"rollup": "2.x.x",
		"rollup-plugin-terser": "7.x.x"
	}
}
